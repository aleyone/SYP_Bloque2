package es.sypbl2.ac1;

public class Actividad1 {

	public static void main(String[] args) {
//		Realiza un programa en Java que dados dos números enteros, 
//		devuelva por pantalla la suma de todos los números que hay entre ellos 
//		(incluyéndolos).
		int procesos=2;
		int parcial1;
		int s1 = 1;
		int s2 = 100;		
		parcial1 = s2 / procesos;
		System.out.print(parcial1);
		//Sumador suma = new Sumador();
		System.out.println(Suma(s1, parcial1));
		System.out.println(Suma((parcial1+1), s2));
	}
	
	public static int Suma (int s1, int s2) {
		int total=0;
		System.out.print("numero 1: "+s1);
		System.out.print("numero 2: "+s2);
		for (int i=s1; s1<s2; i++) {
			//total=total+i;
			System.out.print(i);
		}		
		
		return total;
	}

}
